"w": ["inputwithselected", "unequip", "equipment"],
"R": ["input", ["interact"]],
"r": ["do", [
    ["input", ["interact"]],
    ["input", ["interact", "north"]],
    ["input", ["interact", "south"]],
    ["input", ["interact", "east"]],
    ["input", ["interact", "west"]]]],
"c": ["select", "inventory", 1, true, true],
