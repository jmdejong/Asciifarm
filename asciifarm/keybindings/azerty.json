{
"actions": {
"z": ["move", "north"],
"s": ["move", "south"],
"d": ["move", "east"],
"q": ["move", "west"],
"KEY_UP": ["move", "north"],
"KEY_DOWN": ["move", "south"],
"KEY_RIGHT": ["move", "east"],
"KEY_LEFT": ["move", "west"],
"k": ["move", "north"],
"j": ["move", "south"],
"l": ["move", "east"],
"h": ["move", "west"],
"e": ["inputwithselected", "take", "ground"],
