{
    "width": 24,
    "height": 11,
    "spawn": [17, 8],
    "places": {
        "stairup": [17, 8]
    },
    
    "grid": [
        "      ###############   ",
        "      #++++++++a++++#   ",
        "      #+++++++++++++#   ",
        "      #+a+++++++++++#   ",
        "      #+++++++++++++#   ",
        "      #+++++a+++++++#   ",
        "      ##D######D#####   ",
        "      #++++++#++++++#   ",
        "      #+a++:+#++++<+#   ",
        "      #++++++#++++++#   ",
        "      ###############   "
    ],
    "mapping": {
        ",": "grass",
        "Y": ["grass", "radishplant"],
        "~": "water",
        "#": "wall",
        "X": "rock",
        ".": "ground",
        "+": "floor",
        "^": ["spiketrap"],
        "D": ["ground", "closeddoor"],
        "<": [{
            "type": "roomexit",
            "args": ["begin", "stairdown"],
            "kwargs": {"sprite": "stairup", "size": 1}
        }, "floor"],
        "a": ["floor", {
            "type": "spawner",
            "args": ["rat", 1, 300],
            "kwargs": {"initialSpawn": true, "setHome": true}
        }],
        ":": ["floor", {
            "type": "spawner",
            "args": ["carrotseed", 1, 1200],
            "kwargs": {"initialSpawn": false}
        }],
        " ": []
    }
}
